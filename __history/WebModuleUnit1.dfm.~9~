object WebModule1: TWebModule1
  OldCreateOrder = False
  Actions = <
    item
      Default = True
      Name = 'DefaultHandler'
      PathInfo = '/'
      OnAction = WebModule1DefaultHandlerAction
    end
    item
      Name = 'AllEmployeesAction'
      PathInfo = '/employees'
      OnAction = WebModule1EmployeesActionAction
    end
    item
      Name = 'anEmployee'
      PathInfo = '/employee*'
      OnAction = WebModule1anEmployeeAction
    end
    item
      MethodType = mtPost
      Name = 'CreateEmployee'
      PathInfo = '/create'
      OnAction = WebModule1CreateEmployeeAction
    end>
  Height = 329
  Width = 547
end
